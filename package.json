    "@kong/kauth-client-typescript-axios": "^0.1884.0",
    "@kong/kong-auth-elements": "1.32.4",
    "@kong/kongponents": "8.33.4",
    "@kong/sdk-portal-js": "0.0.1-beta.9",
    "@xstate/vue": "^2.0.0",
    "axios": "0.27.2",
